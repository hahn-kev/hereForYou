{
  "name": "frontend",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json --locale en",
    "start-es": "ng serve --proxy-config proxy.conf.json --aot --locale es --i18n-format xlf --i18n-file src/locale/messages.es.xlf",
    "build-i18n:en": "ng build --prod --aot --output-path ../HereForYou/wwwroot/en --bh /en/ --locale en",
    "build-i18n:es": "ng build --prod --aot --output-path ../HereForYou/wwwroot/es --bh /es/ --locale es --i18n-format xlf --i18n-file src/locale/messages.es.xlf",
    "build-i18n": "npm run build-i18n:en && npm run build-i18n:es",
    "i18n": "ng xi18n --output-path src/locale",
    "build": "npm run build-i18n",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.2",
    "@angular/animations": "^4.4.6",
    "@angular/cdk": "^2.0.0-beta.12",
    "@angular/common": "^4.4.6",
    "@angular/compiler": "^4.4.6",
    "@angular/core": "^4.4.6",
    "@angular/forms": "^4.4.6",
    "@angular/http": "^4.4.6",
    "@angular/material": "^2.0.0-beta.12",
    "@angular/platform-browser": "^4.4.6",
    "@angular/platform-browser-dynamic": "^4.4.6",
    "@angular/router": "^4.4.6",
    "angular-2-local-storage": "^1.0.1",
    "angular2-markdown": "^1.6.0",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "ngx-clipboard": "^8.1.1",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.5.0",
    "@angular/compiler-cli": "^4.4.6",
    "@angular/language-service": "^4.4.6",
    "@types/googlemaps": "^3.29.2",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.90",
    "codelyzer": "~3.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.5.3",
    "protractor": "~5.1.2",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  }
}
